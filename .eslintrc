{
    "env": {
       "browser": true,
       "node": true,
       "es6": true,
       "commonjs": true
    },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 9
  },
  "extends": [ "eslint:recommended", "plugin:react/recommended", "airbnb"],
  "plugins": [ "react" ],
  "rules": {
    "no-console": "off",
    "strict": ["error", "global"],
    "curly": "warn",
    "no-underscore-dangle": "off",
    "jsx-closing-tag-location": "off",
    "react/prop-types": 0,
    "object-curly-spacing": "off"
    }
}
